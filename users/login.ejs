<%- layout('/listings/layouts/boilerplate') %>
<div class="row mt-3">
  
    <div class="col-6 offset-3" >
        <form  method="post" action="/login">
          <div class="mp-3 " class="needs-validation" novalidate>
            <label for="username" class="form-label" >username</label>
            <input name="username"  class="form-control" type="username" id="username" required/>
            <div class="invalid-feedback">Enter username</div>
          </div>
          <div class="mp-3" class="needs-validation" novalidate>
            <label for="password" class="form-label">password</label>
            <input name="password"  class="form-control" type="password"id="password" required/>
            <div class="invalid-feedback">Enter password</div>
          </div>
          <button class="btn btn-success mt-2">Login</button>
          </form>
        </div>
        </div>