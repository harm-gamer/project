<%- layout('/listings/layouts/boilerplate') %>
<div class="row mt-3">
  <h3 class=" col-6 offset-3">Welcome</h3>
    <div class="col-6 offset-3" >
        <form action="/signUp" method="POST">
        <div class="mp-3 "  class="needs-validation" novalidate>
            <label for="username" class="form-label" id="username" >Username</label>
            <input name="username"  class="form-control" type="text" required/>
            <div class="invalid-feedback">Enter Username</div>
          </div>
          <div class="mp-3 ">
            <label for="email" class="form-label" >Email</label>
            <input name="email"  class="form-control" type="email" id="email" required/>
            <div class="invalid-feedback">Enter email</div>
          </div>
          <div class="mp-3">
            <label for="password" class="form-label" >password</label>
            <input name="password"  class="form-control" type="password"id="password" required/>
            <div class="invalid-feedback">Enter password</div>
          </div>
          <button class="btn btn-success mt-2">signup</button>
          </form>
        </div>
        </div>